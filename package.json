{
  "name": "lowcode-components",
  "version": "1.1.3",
  "description": "低代码平台默认组件库",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "dev": "cross-env APP_ROOT=dumi dumi dev",
    "build": "zafuru build",
    "build:types": "tsc --project tsconfig.types.json && tsc-alias -p tsconfig.types.json",
    "build:docs": "cross-env APP_ROOT=dumi dumi build",
    "lint:ts": "tsc",
    "lint:es": "eslint --fix --cache --ext tsx,ts,jsx,js .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepack": "npm run build && npm run build:types"
  },
  "files": [
    "dist",
    "es",
    "lib",
    "typings"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.8.4",
    "@types/react": "^18.0.21",
    "@types/react-css-modules": "^4.6.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@zafuru/plugin-components": "^1.0.6",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.48",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": ">=7.15.0",
    "antd": ">=4.0.0",
    "react": ">=17.0.0"
  }
}
